@model IEnumerable<Product>

<h4>Каталог товаров интернет-магазина товаров</h4>

<a asp-controller="Home" asp-action="Index">Обратно</a>

@foreach (var product in Model)
{
    <div>
        <h3>@product.Name</h3>
        @product.Description
        <h4>@product.Price.ToString("c")</h4>
    </div>
}